module exer04.struct_of_structs_with_defaults {

struct StructOfStruct {
    Foo A = { "d" : 1 };
    Bar B = { "d" : "hw" };
    Fizz c = { "A":{ "d" : 2 }, "B": { "d": "bb"}, "c": "nothing"};
};

struct Foo {
    Int32 d;
};

struct Bar {
    String d = "doe a deer";
};


struct Fizz {
    Foo A;
    Bar B;
    sys.types.Maybe<Fizz> c;
};

};