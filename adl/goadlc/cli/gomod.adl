module cli.gomod {

union GoModule {
    /// The path of the Go module for the generated code.
    GoModResult ModulePath;
    /// Path of a go.mod file
    String GoModFile;
    /// Used to find the go.mod file and calculate the codes module-path and sub-dir of generated code
    String Outputdir;
};

struct GoModResult {
    String ModulePath;
    String RootDir;
};

};