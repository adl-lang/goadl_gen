func AST_{{.Name}}() goadl.ScopedDecl {
	var decl adlast.Decl
	dec := json.NewDecoder(strings.NewReader(`{{.G.JsonEncode .Decl}}`))
	dec.Decode(&decl)
	return goadl.ScopedDecl{
		ScopedName: adlast.ScopedName{
			ModuleName: "{{.ModuleName}}",
			Name:       "{{.Name}}",
		},
		Decl: decl,
	}
}

func init() {
	goadl.RESOLVER.Register(
		adlast.ScopedName{ModuleName: "{{.ModuleName}}", Name: "{{.Name}}"},
		AST_{{.Name}}(),
	)
}
