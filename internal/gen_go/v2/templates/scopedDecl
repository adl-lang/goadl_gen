{{$G := .G}}
{{$name := .Name | .G.GoEscape | public}}
func AST_{{$name}}() {{$G.GoImport "adlast"}}ScopedDecl {
	decl := {{$G.GoDeclValue .Decl}}
	return {{$G.GoImport "adlast"}}ScopedDecl{
		ModuleName: "{{.ModuleName}}",
		Decl: decl,
	}
}

func init() {
	{{$G.GoImport "goadl"}}RESOLVER.Register(
		{{$G.GoImport "adlast"}}ScopedName{ModuleName: "{{.ModuleName}}", Name: "{{.Name}}"},
		AST_{{.Name}}(),
	)
}
